datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  sourceUrl String   @unique
  isUpdated Boolean  @default(false)
  createdAt DateTime @default(now())
}
